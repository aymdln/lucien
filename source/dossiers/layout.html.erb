<html>
	<%= partial 'shared/head' %>
	<body class="page">
		<%= partial("shared/nav", locals: {
			url_image: "../images/logo.jpg"
		}) %>
		<div class="body-wrapper">
			<div class="header-holder center-relative relative">
				<div class="header-logo center-text">
					<a href="/">
						<img src="../images/logo.jpg" alt="Logo">
					</a>
				</div>
				<div class="toggle-holder absolute">
					<div id="toggle">
						<div class="first-menu-line"></div>
						<div class="second-menu-line"></div>
						<div class="third-menu-line"></div>
					</div>
				</div>
				<div class="clear"></div>
			</div>
			<div id="content" class="site-content content-1170 center-relative">
				<article>
					<div class="center-relative clear">
						<div class="entry-content">
							<div class="content-wrap">
								<% images.times do |i| %>
									<div class="one_third margin-0">
										<%= tag :img, src: "../images/dossier_#{id}/#{i + 1}.jpg" %>
										<%# <img src="../images/dossier_1/1.jpg" alt="" /> %>
									</div>
								<% end %>
								<div class="clear"></div>
								<br>
								<br>
								<div class="one_half ">
									<p>
										<b>Project Name:</b>&nbsp;<%= name %><br>
									</p>
									<br>
									<div class="text-left">
										<a href="#" target="_self" class="button ">Download</a>
									</div>
								</div>
								<div class="one_half last ">
									<p><%= description.capitalize %></p>
								</div>
								<div class="clear"></div>
							</div>
						</div>
						<div class="clear"></div>
					</div>
				</article>
			</div>
			<div class="clear"></div>
			<div class="clear"></div>
			<!--Footer-->
			<%= partial 'shared/footer' %>
			<!-- End .body-border -->
		</div>
		<!--Load JavaScript-->
		<script src="../javascripts/jquery.js"></script>
		<script src='../javascripts/jquery.fitvids.js'></script>
		<script src='../javascripts/jquery.smartmenus.min.js'></script>
		<script src='../javascripts/isotope.pkgd.js'></script>
		<script src='../javascripts/imagesloaded.pkgd.js'></script>
		<script src='../javascripts/isotope.pkgd.js'></script>
		<script src='../javascripts/jquery.carouFredSel-6.0.0-packed.js'></script>
		<script src='../javascripts/jquery.mousewheel.min.js'></script>
		<script src='../javascripts/jquery.touchSwipe.min.js'></script>
		<script src='../javascripts/jquery.easing.1.3.js'></script>
		<script src='../javascripts/imagesloaded.pkgd.js'></script>
		<script src='../javascripts/jquery.prettyPhoto.js'></script>
		<script src='../javascripts/jquery.nicescroll.min.js'></script>
		<script src='../javascripts/main.js'></script>
	</body>
</html>
